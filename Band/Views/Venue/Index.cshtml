@{
Layout = "_Layout";
}
  <h2>Venues</h2>

      @if(Model.Count != 0)
      {
          <h5><a class="btn btn-secondary" href="/venues/new">Enter a new venue</a></h5>
          <h5><a class="btn btn-secondary" href="/bands/">Add Venue to Band</a></h5>
          <h5><a class="btn btn-secondary" href="/venues/deleteall">Delete all venues</a></h5>
        <ul>
          @foreach(var venue in Model)
          {
            <li><a href="/venues/@venue.GetId()">@venue.GetVenueName()</li>

          }
        </ul>
      }
      else
      {
        <h5>No venues have been entered.</h5>
        <h5><a class="btn btn-secondary" href="/venues/new">Enter a new venue</a></h5>
        <h5><a class="btn btn-secondary" href="/">Return home</a></h5>
      }
